<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "firma.Comanda"
           table="Comanda">
        <id name = "id" column = "id">
            <generator class="increment"/>
        </id>
        <property name="numeClient" column = "numeClient"/>
        <property name="dataPunereComanda"  type="date" column="dataPunereComanda"/>
        <property name="status" column="status"/>
        <property name="idProdus" column="idProdus"/>
        <property name="cantitateProdus" column="cantitateProdus"/>
        <property name="idAgent" column="idAgent"/>

    </class>

</hibernate-mapping>